{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "^/js/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript; charset=utf-8"
      },
      "dest": "/public/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css; charset=utf-8"
      },
      "dest": "/public/css/$1"
    },
    {
      "src": "^/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/images/$1"
    },
    {
      "src": "^/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "handle": "filesystem"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancellation": true,
    "deploymentEnabled": {
      "main": true,
      "dev": false,
      "preview": false
    }
  }
}
